@using System.Web.Optimization
<!DOCTYPE html>
<html>
    <head>
        <title>ZUBINRAJ.COM</title>
        
        <meta charset="utf-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1" />
        <meta name="apple-mobile-web-app-capable" content="yes" />
        <meta name="apple-mobile-web-app-status-bar-style" content="black" />
        <meta name="format-detection" content="telephone=no"/>
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        
        <!-- Add google analytics stub - ignore while in debug mode -->
        @if (!HttpContext.Current.IsDebuggingEnabled) {
            @Scripts.Render("~/Scripts/analytics.js")
        }
        

        @Styles.Render("~/Content/css")

    </head>
    <body>
        <div id="applicationHost">
            @Html.Partial("_splash")
        </div>

        @*JQuery 2.0 does not support versions prior to IE9 *@
        <!--[if lt IE 9]>
            @Scripts.Render("~/Scripts/jquery-1.9.1.min.js")
        <![endif]-->
        <!--[if gte IE 9]><!-->
            @Scripts.Render("~/Scripts/jquery-2.1.1.min.js")
        <!--<![endif]-->

        @Scripts.Render("~/Scripts/js")


        @if (HttpContext.Current.IsDebuggingEnabled)
        {
            <script type="text/javascript" src="~/Scripts/require.js" data-main="@Url.Content("~/App/main")"></script>
        }
        else
        {
            <script type="text/javascript" src="~/App/main-built.js"></script>
        }

    </body>
</html>